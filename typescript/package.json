{
  "name": "task.json",
  "version": "3.2.0",
  "description": "TypeScript library for task.json",
  "type": "module",
  "keywords": [
    "task.json",
    "typescript",
    "task",
    "todo"
  ],
  "main": "dist/index.js",
  "files": [
    "package*.json",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "type-guard": "ts-auto-guard && sed -i 's/\\.\\/index/.\\/index.js/g' ./src/index.guard.ts",
    "build": "npm run type-guard && rm -rf dist && tsc -p ./tsconfig.build.json",
    "test": "jest"
  },
  "types": "dist/index.d.ts",
  "author": "DCsunset",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/luxon": "^3.2.0",
    "jest": "^29.5.0",
    "ts-auto-guard": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "luxon": "^3.3.0"
  }
}
